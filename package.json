        "axios": "^0.19",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-vue": "^6.2.2",
        "laravel-mix": "^5.0.1",
        "prettier": "^1.19.1",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
